{
  "connection": "default",
  "collectionName": "group_invites",
  "info": {
    "name": "groupInvite"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "sent",
        "accepted",
        "declined"
      ]
    },
    "food_plan": {
      "model": "food-plan"
    },
    "from": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
