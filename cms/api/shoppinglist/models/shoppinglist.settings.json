{
  "connection": "default",
  "collectionName": "shoppinglists",
  "info": {
    "name": "Shoppinglist"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "date": {
      "type": "date",
      "required": true
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "shoppinglists"
    },
    "sharedWith": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "shoppinglist"
    },
    "recipe_items": {
      "unique": true,
      "collection": "recipe-item"
    }
  }
}
